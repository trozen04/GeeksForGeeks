class Solution
{
    ArrayList<Integer> findMissing(int a[], int b[], int n, int m)
    {
        ArrayList<Integer> list = new ArrayList<>();
        for(int i = 0; i < n; i++){
            int flag = 0;
            for(int j = 0; j < m; j++){
                if(a[i] == b[j]){
                    flag = 1;
                    break;
                }
            }
            if(flag == 0){
                list.add(a[i]);
            }
        }
        return list;
    }
}
